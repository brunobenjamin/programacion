// lab.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
//1+3+5+7+...+n
void main()
{int n,s,i;
cout<<"hasta que numero desea sumar"<<endl;
cin>>n;
s=0;
i=0;
while(i<=n)
{s=i+s;
i=i+1;
}
cout<<"la suma es";
cout<<s<<endl;
getch();

}

